- void css(string file)
  link(rel='stylesheet', type='text/css', href='#{file}')

- void favicon(string file)
  link(rel='shortcut icon', type='image/png', href='#{file}')

doctype 5
html
  head
    meta(charset='UTF-8')
    title #{GLOBALS.WebsiteName}
    block styling
  body
    #wrapper
      include header
      block body
      include footer
