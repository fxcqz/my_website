- void css(string file)
  link(rel='stylesheet', type='text/css', href='css/#{file}.css')

doctype 5
html
  head
    title #{GLOBALS.WebsiteName}
    - css("main");
  body
    h1 #{ctx.heading}
    h2 Posts
    - foreach (post; ctx.posts)
      ul
        li
          a(href="#{post.url}") #{post.obj.title}
